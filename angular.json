{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mattress-finder": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mattress-finder",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "extractCss": true,
            "es5BrowserSupport": true,
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              {
                "input": "src/styles.scss",
                "lazy": true,
                "bundleName": "styles"
              },
              {
                "input": "src/assets/externals/fonts/lato.css",
                "lazy": true,
                "bundleName": "fonts-lato"
              },
              {
                "input": "src/assets/externals/fonts/raleway.css",
                "lazy": true,
                "bundleName": "fonts-raleway"
              },
              {
                "input": "src/assets/externals/material.indigo-pink.min.css",
                "lazy": true,
                "bundleName": "indigo-pink"
              },
              {
                "input": "node_modules/bootstrap/dist/css/bootstrap.min.css",
                "lazy": true,
                "bundleName": "bootstrap-css"
              },
              {
                "input": "node_modules/nouislider/distribute/nouislider.min.css",
                "lazy": true,
                "bundleName": "nouislider"
              }             
            ],
            "scripts": [
              {
                "input": "node_modules/popper.js/dist/umd/popper.min.js",
                "lazy": true,
                "bundleName": "popper"
              },
              {
                "input": "node_modules/bootstrap/dist/js/bootstrap.min.js",
                "lazy": true,
                "bundleName": "bootstrap"
              }
            ],
            "lazyModules": [
              "src/app/footer/footer.module"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "es5": {
              "tsConfig": "./tsconfig.es5.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mattress-finder:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "mattress-finder:build:production"
            },
            "es5": {
              "browserTarget": "mattress-finder:build:es5"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mattress-finder:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mattress-finder:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mattress-finder:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "mattress-finder"
}